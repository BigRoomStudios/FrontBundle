	{% block head %}
		
		<head>
			
			{# Meta tags #}
			{% block meta %}
				<meta charset="utf-8">
		    	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
		    	<meta name="viewport" content="width=device-width, initial-scale=1">
	    	{% endblock %}
			
	    	{# Page name #}
			{% block title %}<title>{% block page_title %}{% endblock %} - {% block site_title %}{% endblock %}</title>{%endblock%}
			{% block metadescription %}<meta name="description" content="{% block description %}{% endblock %}">{% endblock %}
			
			{# Stylesheets #}
			{% block stylesheets %}{% endblock %}
			
			{# Favicons #}
			{% block favicon %}
			<link rel="shortcut icon" href="{{ asset('favicon.ico') }}">
			{% endblock %}
			
			{# Javascripts #}
			{% block javascripts %}
				{% javascripts
					'bundles/brsfront/js/modernizr.custom.96376.js'
				%}
				<script src="{{ asset_url }}"></script>
				{% endjavascripts %}
				<!--[if lte IE 8]><script src="/bundles/brsfront/js/rem.min.js"><![endif]-->
			{% endblock %}
			
		</head>
		
	{% endblock %}
	
	{% if app.debug %}
		<!--
			{% block templates %}
				BRSFrontBundle:Frame:default.html.twig
			{% endblock %}
		-->
	{% endif %}
	
	{% block body %}
		
		<body>
			
			{% block header %}
				
				<header class="toolbar" role="navigation">
					
					<div class="wrapper">
						
						<div class="brand">
							<a href="{{ path('home') }}" class="brand-name">BRS Core</a>
						</div>
						
						<div class="toolbar-actions">
							<a href="#" class="button hollow rounded primary">Create an account</a>
							<a href="#" class="button hollow rounded secondary">Login</a>
						</div>
						
					</div>
					
				</header>
				
			{% endblock header %}
			
			{% block body_content %}
				
			{% endblock %}
			
			{% block footer %}
				
				<footer class="toolbar" role="navigation">
					
					<div class="wrapper">
						
						copy write info
						
					</div>
					
				</footer>
				
			{% endblock footer %}
			
			{% block bottomscripts %}
				
				{% javascripts 'bundles/brsfront/js/jquery.2.0.3.min.js' %}
					<script src="{{ asset_url }}"></script>
				{% endjavascripts %}
				
			{% endblock %}
			
		</body>
		
	{% endblock %}
	
</html>